<h3>Here are all the items in this category:</h3>

<ol>
@foreach (var cd in @Model["CDs"])
{
  <li><a href='/categories/@Model["category"].GetCategoryId()/cds/@cd.GetId()'>@cd.GetDescription()</a></li>
}
</ol>

<p><a href='/categories/@Model["category"].GetCategoryId()/cds/new'>Add a new item</a></p>
